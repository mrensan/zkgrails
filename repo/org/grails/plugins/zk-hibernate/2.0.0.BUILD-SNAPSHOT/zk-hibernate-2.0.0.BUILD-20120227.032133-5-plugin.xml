<plugin name='zk-hibernate' version='2.0.0.BUILD-SNAPSHOT' grailsVersion='2.0 &gt; *'>
  <author>Chanwit Kaewkasi</author>
  <authorEmail>chanwit@gmail.com</authorEmail>
  <title>Hibernate support for ZKGrails</title>
